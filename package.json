    "eslint-plugin-babel": "^4.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "flickr-simple-reorder-server": "github:whitetrefoil/flickr-simple-reorder-server#v0.1.11",
    "fs-extra": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy": "^1.16.2",
    "jasmine-core": "^2.8.0",
    "jest": "^21.2.1",
    "less-loader": "^4.0.5",
    "lodash-webpack-plugin": "^0.11.4",
    "meow": "^3.7.0",
    "merge-stream": "^1.0.1",
    "mock-server-middleware": "^0.4.0-alpha.4",
    "prerender-spa-plugin": "^2.1.0",
    "require-uncached": "^1.0.3",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.0",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.3",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-stream": "^4.0.0"
  },
  "engines": {
    "node": ">=8.6.0",
    "npm": ">=5.4.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/style-mock.js"
    },
    "setupFiles": [
      "babel-polyfill/lib/index.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}

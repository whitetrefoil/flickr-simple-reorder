    "less": "^2.7.3",
    "lodash": "^4.17.4",
    "store": "^2.0.12",
    "superagent": "^3.8.0",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.6",
    "@types/connect": "^3.4.31",
    "@types/core-js": "^0.9.43",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.0",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/fs-extra": "^4.0.3",
    "@types/gulp": "^4.0.5",
    "@types/gulp-connect": "^5.0.0",
    "@types/gulp-util": "^3.0.32",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/http-proxy": "^1.12.2",
    "@types/jest": "^21.1.5",
    "@types/lodash": "^4.14.80",
    "@types/lodash-webpack-plugin": "^0.11.1",
    "@types/meow": "^3.6.2",
    "@types/node": "^8.0.47",
    "@types/run-sequence": "0.0.29",
    "@types/semver": "^5.4.0",
    "@types/superagent": "^3.5.6",
    "@types/webpack": "^3.0.14",
    "@types/webpack-dev-server": "^2.9.1",
    "ajv": "^5.3.0",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "clear-require": "^2.0.0",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "flickr-simple-reorder-server": "github:whitetrefoil/flickr-simple-reorder-server",
    "fs-extra": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy": "^1.16.2",
    "jasmine-core": "^2.8.0",
    "jest": "^21.2.1",
    "less-loader": "^4.0.5",
    "lodash-webpack-plugin": "^0.11.4",
    "meow": "^3.7.0",
    "merge-stream": "^1.0.1",
    "mock-server-middleware": "^0.4.0-alpha.4",
    "prerender-spa-plugin": "^2.1.0",
    "require-uncached": "^1.0.3",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.0",
    "semver": "^5.4.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.1.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-stream": "^4.0.0"
  },
  "engines": {
    "node": ">=8.8.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": false,
        "useBabelrc": true
      }
    },
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/style-mock.js",
      "\\.vue$": "<rootDir>/tests/vue-mock.js",
      "iview/src/components/.*(?!\\.[jt]s)": "<rootDir>/tests/vue-mock.js"
    },
    "setupFiles": [
      "babel-polyfill/lib/index.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!iview).*/"
    ]
  }
}

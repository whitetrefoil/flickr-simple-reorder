{
  "name": "@whitetrefoil/flickr-simple-reorder",
  "version": "2.2.0-alpha.1",
  "description": "A simple tool to help reorder photos in galleries.",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/whitetrefoil/flickr-simple-reorder.git"
  },
  "homepage": "https://github.com/whitetrefoil/flickr-simple-reorder",
  "author": "WhiteTrefoil <whitetrefoil@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "flickr"
  ],
  "scripts": {
    "build": "gulp build",
    "start": "gulp serve -d",
    "serve": "gulp serve -d",
    "test": "rimraf test_results && tslint -p tsconfig.json"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.42",
    "@whitetrefoil/debug-log": "^0.1.1",
    "av-ts": "^0.9.3",
    "debug": "^3.1.0",
    "eventemitter3": "^3.0.1",
    "fancy-log": "^1.3.2",
    "iview": "^2.11.0",
    "lodash": "^4.17.5",
    "store": "^2.0.12",
    "superagent": "^3.8.2",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@types/body-parser": "^1.16.8",
    "@types/clear-require": "^2.0.0",
    "@types/connect": "^3.4.31",
    "@types/core-js": "^0.9.46",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.0",
    "@types/extract-text-webpack-plugin": "^3.0.2",
    "@types/fancy-log": "^1.3.0",
    "@types/gulp": "^4.0.5",
    "@types/gulp-connect": "^5.0.2",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/http-proxy": "^1.16.0",
    "@types/lodash": "^4.14.105",
    "@types/lodash-webpack-plugin": "^0.11.3",
    "@types/node": "^9.4.7",
    "@types/run-sequence": "0.0.30",
    "@types/semver": "^5.5.0",
    "@types/superagent": "^3.5.7",
    "@types/webpack": "^4.1.1",
    "@types/webpack-dev-server": "^2.9.4",
    "@whitetrefoil/flickr-simple-reorder-server": "^0.2.3",
    "ajv": "^6.3.0",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-lodash": "^3.3.2",
    "clear-require": "^2.0.0",
    "core-js": "^2.5.3",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.5.0",
    "gulp-plumber": "^1.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "http-proxy": "^1.16.2",
    "jasmine-core": "^3.1.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lodash-webpack-plugin": "^0.11.4",
    "meow": "^4.0.0",
    "merge-stream": "^1.0.1",
    "mock-server-middleware": "^0.4.0-alpha.4",
    "raw-loader": "^0.5.1",
    "require-uncached": "^1.0.3",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.1",
    "semver": "^5.5.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.7.2",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.2.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^3.1.1"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tests/tsconfig.json",
        "skipBabel": false,
        "useBabelrc": false
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^vue$": "<rootDir>/node_modules/vue/dist/vue.runtime.esm.js",
      "^vue-router$": "<rootDir>/node_modules/vue-router/dist/vue-router.esm.js",
      "^vuex$": "<rootDir>/node_modules/vuex/dist/vuex.esm.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.ts",
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/style-mock.ts"
    },
    "setupFiles": [
      "@babel/polyfill",
      "jest-localstorage-mock",
      "<rootDir>/tests/unhandled-promise-rejection.ts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup.ts",
    "testEnvironment": "jest-environment-jsdom-global",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!vue)",
      "<rootDir>/node_modules/(?!iview).*/"
    ]
  }
}

  "description": "A simple tool to help reorder photos in galleries.",
  "author": "WhiteTrefoil <whitetrefoil@gmail.com>",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/whitetrefoil/flickr-simple-reorder.git"
  },
  "homepage": "https://github.com/whitetrefoil/flickr-simple-reorder",
  "license": "Apache-2.0",
  "keywords": [
    "flickr"
  ],
  "scripts": {
    "build": "gulp build",
    "gulp": "gulp",
    "start": "gulp serve -d",
    "test": "rimraf test_results && jest --no-cache"
  },
  "dependencies": {
    "av-ts": "^0.9.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.1.0",
    "debug": "^3.1.0",
    "iview": "^2.4.0",
    "less": "^2.7.2",
    "lodash": "^4.17.4",
    "semver": "^5.4.1",
    "store": "^2.0.12",
    "superagent": "^3.6.1",
    "vue": "^2.4.4",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.1",
    "vuex-router-sync": "^4.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.0",
    "@types/fs-extra": "^4.0.2",
    "@types/jest": "^21.1.1",
    "@types/lodash": "^4.14.76",
    "@types/node": "^8.0.31",
    "@types/semver": "^5.4.0",
    "@types/superagent": "^3.5.5",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
